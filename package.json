{
  "name": "@intusfacultas/component-library",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/IntusFacultas/NPM_REPO",
    "directory": "packages/component-library"
  },
  "scripts": {
    "build": "sh scripts/build.sh",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "build:component": "cross-env NODE_ENV=production rollup --config \"scripts/rollup.component.config.js\"",
    "build:index": "cross-env NODE_ENV=production rollup --config \"scripts/rollup.index.config.js\"",
    "build:index:esm": "cross-env NODE_ENV=production rollup --config \"scripts/rollup.index.esm.config.js\"",
    "build:library": "cross-env NODE_ENV=production rollup --config scripts/rollup.config.js",
    "build:vue": "vue-cli-service build --skip-plugins @vue/cli-plugin-eslint --target lib --name IFCComponentLibrary index.esm.js ",
    "prebuild": "rimraf ./dist && rimraf ./build",
    "storybook": "start-storybook -p 6006 -s ./public",
    "test": "jest --coverage && open-cli coverage/lcov-report/index.html -- firefox",
    "test:coverage": "jest --coverage && open-cli coverage/lcov-report/index.html",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json --watch"
  },
  "main": "build/index.esm.js",
  "module": "build/index.esm.js",
  "browser": "dist/component-library.esm.js",
  "files": [
    "dist/*",
    "build/**/*.esm.js",
    "build/**/*.js",
    "build/**/*.svg",
    "src/**/*.vue",
    "src/**/*.js"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.8.7",
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@storybook/addon-a11y": "^6.3.6",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-jest": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storyshots": "^6.3.6",
    "@storybook/vue": "^6.3.6",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.9.0",
    "jest-vue-preprocessor": "^1.7.1",
    "node-sass": "^7.0.0",
    "open-cli": "^7.0.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.6",
    "rollup-plugin-vue-inline-svg": "^1.1.2",
    "sass": "^1.36.0",
    "sass-loader": "^10.2.0",
    "vue-cli-plugin-svg": "~0.1.3",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "moduleFileExtensions": [
    "js",
    "mjs"
  ]
}
